@page
@model IndexModel
@{
    ViewData["Title"] = "YubiKey Self Service";
}

<div class="text-center">
	<h1 class="display-4">YubiKey Self Service</h1>
</div>

<div class="text-center">
	<form method="post">
		<input type="text" name="username" placeholder="username" asp-for="Username" /><br /><br />
		<input type="text" name="serialnumber" placeholder="YubiKey serial number" asp-for="Serialnumber" /><br /><br />
		<input type="text" name="yotpcode" placeholder="Yubico OTP code" asp-for="YOTPcode" /> <br/> <br/>
		<input type="submit">
	</form>
</div>
<br />
<br />
<div class="text-center">
	<p class="alert-info">@Model.Result</p>
</div>